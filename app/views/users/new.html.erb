<% provide(:title, 'Sign up') %>

<h1>New User Sign-up</h1>


<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@user) do |f| %>


        <%= render 'shared/error_messages' %>


        <%= render 'fields' %>
        <%= f.submit "Create my account", class: "btn btn-primary" %>

    <% end %>


<%= link_to 'Back', users_path %>
